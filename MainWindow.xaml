<Window x:Class="Practice_IComparable.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practice_IComparable"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <ListBox x:Name="lbList" Height="326" Width="214" d:ItemsSource="{d:SampleData ItemCount=5}" Canvas.Left="400" Canvas.Top="41"/>
        <Button x:Name="btnOriginal" Content="Original Sorting" Canvas.Left="108" Canvas.Top="58" HorizontalAlignment="Center" VerticalAlignment="Top" Width="138" Click="btnOriginal_Click"/>
        <Button x:Name="btnBrand" Content="Brand" Canvas.Left="108" Canvas.Top="136"  HorizontalAlignment="Center" VerticalAlignment="Top" Width="138" Click="btnBrand_Click"/>
        <Button x:Name="btnQuantityDesc" Content="Quantity Descending" Canvas.Left="108" Canvas.Top="217" HorizontalAlignment="Left" VerticalAlignment="Center" Width="138" Click="btnQuantityDesc_Click"/>
        <Button x:Name="btnThicknessAsc" Content="Thickness Ascending" Canvas.Left="108" Canvas.Top="307" HorizontalAlignment="Center" VerticalAlignment="Top" Width="138" Click="btnThicknessAsc_Click"/>

    </Canvas>
</Window>
